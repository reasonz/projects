.tui_panel_wrap .tui_panel_w_br { bottom: -4px; right: -4px; background-position: -5px -5px; }

.tui_panel_wrap { _border-bottom: 4px solid #e9e9e9; }
.tui_panel_wrap .tui_panel_w_bm,
.tui_panel_wrap .tui_panel_w_bl,
