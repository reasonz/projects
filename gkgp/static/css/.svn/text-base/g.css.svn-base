/* play and comment counts */
.pack .d_nums span { font-size: 11px; cursor: default; }
.pack .d_nums .d_play { padding: 0 7px 0 15px; background: url(img/ui/pack/i_v6.png) -119px -108px no-repeat; }
.pack .d_nums .d_cmt { padding: 0 0 0 19px; background: url(img/ui/pack/i_v6.png) -118px -128px no-repeat; }

/* black transparent layer */
.pack .pic .vpbg { width: 132px; height: 20px;  background: #000; opacity: 0.6; filter: alpha(opacity = 60); position: absolute; left: 0; bottom: 0; }
.pack_plist_card .pic .vpbg,
.pack_plist_brief .pic .vpbg { width: 128px; left: 0; bottom: 4px; }
.pack_playlist_card .pic .vpbg,
.pack_playlist_brief .pic .vpbg { width: 128px; left: 1px; bottom: 5px; }
.pack .pic .vinf { width: 127px; height: 20px; padding: 0 0 0 5px; line-height: 21px; overflow: hidden; position: absolute; bottom: 0; left: 0; margin: 0; color: #fff; text-decoration: none }
.pack_plist_card .pic .vinf,
.pack_plist_brief .pic .vinf { width: 123px; left: 0; bottom: 4px; }
.pack_playlist_card .pic .vinf,
.pack_playlist_brief .pic .vinf { width: 123px; left: 1px; bottom: 5px; }
/* IE6 1px gap fix */
* .pack .pic .vpbg,
* .pack .pic .vinf { _bottom: -1px; }
* .pack_plist_card .pic .vpbg,
* .pack_plist_card .pic .vinf,
* .pack_plist_brief .pic .vpbg,
* .pack_plist_brief .pic .vinf { _bottom: 3px; }
* .pack_playlist_card .pic .vpbg,
* .pack_playlist_card .pic .vinf,
* .pack_playlist_brief .pic .vpbg,
* .pack_playlist_brief .pic .vinf { _bottom: 4px; }

/* 2012 new playlist pack */
.pack.pack_plist_card { width: 132px; }
.pack_plist_card .pic,
.pack_plist_brief .pic { width: 128px; height: 95px; padding: 0 4px 4px 0; background: url(/skin/__g/img/ui/pack/i_v6.png) 0 -400px no-repeat; }
.pack_plist_card .inner, 
.pack_plist_brief .inner { width: 128px; height: 95px; overflow: hidden; position: relative; }
.pack_plist_card .pic img.pack_listImg,
